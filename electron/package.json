{
  "name": "Iron",
  "version": "0.1.10",
  "private": true,
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "emoji-mart": "^2.11.0",
    "phoenix-channels": "^1.0.0",
    "qs": "~6.5.2",
    "react": "^16.5.2",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.5.2",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "yarn build --em.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "dist": "electron-builder -mw",
    "lint": "eslint ."
  },
  "main": "public/electron.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "4.1.1",
    "electron-builder": "^20.34.0",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-react": "^7.12.4",
    "wait-on": "^3.2.0"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "NSIS"
    }
  },
  "author": "Gary Button",
  "description": "End to end encrypted messaging app",
  "homepage": "./"
}
