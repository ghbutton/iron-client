{
  "name": "iron",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "phoenix-channels": "^1.0.0",
    "qs": "~6.5.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "yarn build --em.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "dist": "electron-builder"
  },
  "main": "public/electron.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^3.0.9",
    "electron-builder": "^20.34.0",
    "wait-on": "^3.2.0"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "author": "Gary Button",
  "description": "End to end encrypted messaging app",
  "homepage": "./"
}
